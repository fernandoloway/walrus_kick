<% include partial/html_head.ejs %>
</head>

<body>
    <% include partial/navbar.ejs %>

    <div class="container-fluid" id="container">
      <div class="row">
        <% include partial/dashboard_sidebar_season.ejs %>

        <main class="col-sm-9 offset-sm-3 col-md-10 offset-md-2 pt-3">
          <h2>Daftar Invoice</h2>
          <div class="table-responsive">
            <table class="table table-striped">
              <thead> 
                <tr>
                  <th>Product Name</th>
                  <th>Product Color</th>
                  <th>Product Size</th>
                  <th>Quantity</th>
                </tr>
              </thead>
              <tbody>
              <% data.forEach (x => { %>
                <tr>
                  <td><%= x.ProductName %></td>
                  <td><%= x.ProductColor %></td>
                  <td><%= x.ProductSize %></td>
                  <td><%= x.Quantity %></td>
                </tr>
              <% }) %>
              </tbody>
            </table>
          </div>
        </main>
      </div>
    </div>


    <% include partial/libs_location.ejs %>
    <% include partial/script_clickable_row.ejs %>

</body>
</html>